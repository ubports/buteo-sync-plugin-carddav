#!/usr/bin/make -f
# -*- makefile -*-
export DPKG_GENSYMBOLS_CHECK_LEVEL=4

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

override_dh_install:
	# remove the tests
	rm -rf debian/tmp/opt
	# Add account files
	mkdir -p debian/buteo-sync-plugins-contacts-carddav/usr/share/accounts/services/
	cp data/ubports/account-services/* \
		debian/buteo-sync-plugins-contacts-carddav/usr/share/accounts/services/
	dh_install --fail-missing

%:
	dh $@ --parallel --fail-missing 
